        $("#cpOverlay").show();
    });


    $("#generatedCode").on("focus", function () {
        clearInterval(document.updater);
        if (this.isSelected)
            return false;

        this.select();
        this.isSelected = true;
    }).on("blur", function () {
        this.isSelected = false;
