    style += ".mbMiniPlayer." + skinName + " .playerTable{background-color:transparent; border-radius:" + borderRadius + "px !important;}\n";
    var fontsize = 20 + parseFloat(playerPadding);
    var timeFontSize = fontsize <= 22 ? 12 : fontsize - 10;

    style += ".mbMiniPlayer." + skinName + " .playerTable span{background-color:" + main[0] + "; padding:" + (parseFloat(playerPadding) + 3) + "px !important; font-size: " + fontsize + "px;}\n";
    style += ".mbMiniPlayer." + skinName + " .playerTable span.map_time{ font-size: " + (timeFontSize) + "px !important; width: 50px !important}\n";

