        onSlide: function (o) {
            $("#" + o.id + "_val").val($(o).mbgetVal());
        }
    });

    function showPopUp(txt) {
        var popUp = $("<div/>").addClass("popup");
        $("body").append(popUp);

        popUp.html(txt);

        setTimeout(function () {
            popUp.fadeOut(1000);

